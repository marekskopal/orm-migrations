{
    "name": "marekskopal/orm-migrations",
    "type": "library",
    "description": "Migrations for MarekSkopal ORM.",
    "keywords": [
        "orm",
        "migrations",
        "sql",
        "mysql",
        "sqlite"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marek Skopal",
            "homepage": "https://www.marekskopal.cz"
        }
    ],
    "require": {
        "php": ">=8.4",
        "marekskopal/orm": "dev-main",
        "nette/php-generator": "^4.1",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpstan/extension-installer": "^1.4",
        "phpunit/phpunit": "^11.4",
        "slevomat/coding-standard": "^8.15",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "MarekSkopal\\ORM\\Migrations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarekSkopal\\ORM\\Migrations\\Tests\\": [
                "tests/"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "8.4"
        },
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
